{
  "development": {
    "image": "zojeda/mongo-training",
    "code": {
      "path": "/capacitacion-mongo",
      "bindToHostPath": "./",
      "provisions": [
        {
          "name": "fsCopy",
          "params": {
            "path": "./"
          }
        }
      ]
    },
    "tools": {
      "cloud9": {
        "command": "node /cloud9/server.js --listen 0.0.0.0 -a : -w /capacitacion-mongo",
        "description": "Cloud9 IDE",
        "port": 8181,
        "type": "web-application"
      }
    },
    "services": {
      "mongodb": {
        "image": "mongo:3.2.6",
        "port": 27017,
        "type": "tcp-service"
      }
    }
  },
  "team": "mongo.capa.hx"
}